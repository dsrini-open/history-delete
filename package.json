{
  "name": "history-delete",
  "version": "1.1.0",
  "description": "dsrini.open history-delete project",
  "main": "index.js",
  "author": "dsrini.open",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=development gulp",
    "build:opera": "NODE_ENV=development TARGET_ENV=opera gulp",
    "build:chrome": "NODE_ENV=development TARGET_ENV=chrome gulp",
    "build:prod": "NODE_ENV=production gulp",
    "build:prod:opera": "NODE_ENV=production TARGET_ENV=opera gulp",
    "build:prod:chrome": "NODE_ENV=production TARGET_ENV=chrome gulp",
    "build:prod:zip": "yarn run build:prod && yarn run ext_zip",
    "build:prod:zip:opera": "yarn run build:prod:opera && yarn run ext_zip",
    "build:prod:zip:chrome": "yarn run build:prod:chrome && yarn run ext_zip",
    "ext_zip": "web-ext build -s dist/ -a artifacts/",
    "ext_run": "web-ext --verbose run -s dist/",
    "ext_check": "web-ext lint",
    "exec": "yarn run build && yarn run ext_run",
    "exec:prod": "yarn run build:prod && yarn run ext_run",
    "inspect": "webpack --profile --json > report.json; webpack-bundle-analyzer report.json --mode static && sleep 6 && rm report.{json,html}",
    "update": "ncu --upgrade --upgradeAll && yarn",
    "release": "standard-version && yarn run push"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-jsonmin": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sequence": "^1.0.0",
    "ignore-loader": "^0.1.2",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "typeface-noto-sans": "^0.0.54",
    "vue-loader": "^14.2.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "web-ext": "^2.6.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "psl": "^1.1.25",
    "vue": "^2.5.16",
    "vuetify": "^1.0.10",
    "webextension-polyfill": "^0.2.1"
  }
}
